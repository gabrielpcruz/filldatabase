{
    "name": "gabrielpcruz/slim",
    "description": "A helfull tool for populate a database",
    "minimum-stability": "stable",
    "license": "proprietary",
    "authors": [
        {
            "name": "Gabriel Cruz",
            "email": "gabrielcruz.mail@gmail.com"
        }
    ],
    "scripts": {
        "test": "php vendor/bin/infection --test-framework=codeception --threads=12"
    },
    "require": {
        "slim/slim": "^4.10",
        "guzzlehttp/psr7": "^2.4",
        "php-di/php-di": "^6.4",
        "adbario/php-dot-notation": "^3.1",
        "slim/twig-view": "^3.3",
        "slashtrace/slashtrace": "^1.1",
        "fig/http-message-util": "^1.1",
        "illuminate/database": "^8.83",
        "symfony/console": "^5.4",
        "symfony/yaml": "^5.4",
        "league/oauth2-server": "^8.3",
        "slim/flash": "^0.4.0",
        "fakerphp/faker": "^1.20",
        "ext-json": "*",
        "ext-yaml": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.0.0",
        "infection/infection": "0.26.6",
        "infection/codeception-adapter": "^0.4.1",
        "roave/infection-static-analysis-plugin": "^1.18"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
